    <script>
        $(document).ready(function() {
            $('#contact_us_btn').on('click', function(){
                if($("#cu_email").val() === ''){
                    toast({
                        type: 'error',
                        title: "Please insert email!"
                    });
                    return false;
                }
                var formData = {
                        "fields":[
                            {
                                "name": "firstname",
                                "value": $("#cu_first_name").val()
                            },{
                                "name": "lastname",
                                "value": $("#cu_last_name").val()
                            },{
                                "name": "email",
                                "value": $("#cu_email").val()
                            },{
                                "name": "mobilephone",
                                "value": $("#cu_mobile_num").val()
                            },{
                                "name": "i_m_a",
                                "value": $("#cu_i_am").val()
                            },{
                                "name": "country",
                                "value": $("#cu_country").val()
                            },{
                                "name": "message",
                                "value": $("#cu_message").val()
                            }
                        ]
                    }
                $.ajax({
                    type: 'post',
                    url: 'https://api.hsforms.com/submissions/v3/integration/submit/4363226/3cbcacb7-8d3a-445e-a920-0abda002a4fc',
                    contentType: "application/json",
                    data: JSON.stringify(formData),
                    success: function (data) {
                        toast({
                            type: 'success',
                            title: "Thanks your message has successfuly sent."
                        });
                        $("#cu_first_name").val("");
                        $("#cu_last_name").val("");
                        $("#cu_email").val("");
                        $("#cu_mobile_num").val("");
                        $("#cu_i_am").val("Brand");
                        $("#cu_country").val("Australia");
                        $("#cu_message").val("");
                    },
                    error: function (data) {
                        toast({
                            type: 'error',
                            title: "failed to send your message!"
                        });
                    },
                });
            })
            var countryArr = [
