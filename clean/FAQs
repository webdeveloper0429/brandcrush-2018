    <script>
        $(document).ready(function(){
            
            $(".js-modal-btn").modalVideo({channel:'vimeo'});

            var generalStr = ''
            generalArr.map((item)=>{
                generalStr += `
                            <div class="faq-item-w">
                                <div class="question">
                                    ${item.question}
                                </div>
                                <div class="answer">
                                    <p>
                                        ${item.answer}
                                    </p>
                                </div>
                            </div>
                            <hr/>
                `
            })
            $('#content_general').html(generalStr);

            var brandStr = ''
            brandArr.map((item)=>{
                brandStr += `
                            <div class="faq-item-w">
                                <div class="question">
                                    ${item.question}
                                </div>
                                <div class="answer">
                                    <p>
                                        ${item.answer}
                                    </p>
                                </div>
                            </div>
                            <hr/>
                `
            })
            $('#content_brand').html(brandStr);

            var activationStr = ''
            activationArr.map((item)=>{
                activationStr += `
                            <div class="faq-item-w">
                                <div class="question">
                                    ${item.question}
                                </div>
                                <div class="answer">
                                    <p>
                                        ${item.answer}
                                    </p>
                                </div>
                            </div>
                            <hr/>
                `
            })
            $('#content_activation').html(activationStr);

            var consumerStr = ''
            consumerArr.map((item)=>{
                consumerStr += `
                            <div class="faq-item-w">
                                <div class="question">
                                    ${item.question}
                                </div>
                                <div class="answer">
                                    <p>
                                        ${item.answer}
                                    </p>
                                </div>
                            </div>
                            <hr/>
                `
            })
            $('#content_consumer').html(consumerStr);

            ////collapse-start////
            $('.question').click(function(){
                $('.question').removeClass('opened');
                $('.answer').css('height', '0');
                $(this).addClass('opened');
                var panel = $(this).next()[0];
                panel.style.height = panel.scrollHeight + "px";
            })
            ////collapse-end////
            
            ////tab-start////
            $('.tab_content').hide();
            $('.tab_content:first').show();
            $('.tab-btn').click(function(){
                var t = $(this).attr('id');
                if(!$(this).hasClass('actived-btn')){
                    $('.tab-btn').removeClass('actived-btn');
                    $(this).addClass('actived-btn');
                    $('.tab_content').hide();
                    $('#content_'+t).fadeIn('slow');
                }
            })
            ////tab-end////
        })
    </script>
